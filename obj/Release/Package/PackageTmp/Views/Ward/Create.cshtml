@model List<HosSystem.Models.Patient>

@{
    Layout = null;
}

<!DOCTYPE html>

<html style="height: auto; min-height: 100%;">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>AdminLTE 2 | User Profile</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="~/bower_components/bootstrap/dist/css/bootstrap.min.css">

    <!-- Font Awesome -->
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/bower_components/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/bower_components/Ionicons/css/ionicons.min.css">
    <!-- jvectormap -->
    <link rel="stylesheet" href="~/bower_components/jvectormap/jquery-jvectormap.css">
    <!-- Theme style -->
    <!-- Theme style -->
    <link rel="stylesheet" href="~/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/dist/css/skins/_all-skins.min.css">
    <!-- bootstrap datepicker -->
    <link rel="stylesheet" href="~/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">
    <!-- Bootstrap time Picker -->
    <link rel="stylesheet" href="~/plugins/timepicker/bootstrap-timepicker.min.css">

    <!-- Morris charts -->
    <link rel="stylesheet" href="~/bower_components/morris.js/morris.css">


    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="skin-blue sidebar-mini" style="height: auto; min-height: 100%;">
    <div class="wrapper" style="height: auto; min-height: 100%;">

        <header class="main-header">
            <!-- Logo -->
            <a href="#" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"></span>
                <!-- logo for regular state and mobile devices -->
                @*<span class="logo-lg">ระบบหอผู้ป่วย</span>*@
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>

                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/dist/img/user2-160x160.jpg" class="user-image" alt="User Image">
                                <span class="hidden-xs">@Session["UserName"] </span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="~/dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">

                                    <p>

                                        <small>@Session["UserName"] </small>
                                    </p>
                                </li>
                                <!-- Menu Body -->
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    @*<div class="pull-left">
                                            <a href="#" class="btn btn-default btn-flat">Profile</a>
                                        </div>*@
                                    <div class="pull-right">
                                        <a href="@Url.Action("Logout","Ward")" class="btn btn-default btn-flat">Sign out</a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="~/dist/img/ptlLogo.jpg" class="img-responsive" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>ระบบหอผู้ป่วย</p>
                        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>


                <ul class="sidebar-menu tree" data-widget="tree">


                    <div class="box-body">


                        <div class="box" style="background-color:lightgray; width:245px">
                            <div class="box-header">
                                <h3 class="box-title">รายชื่อผู้ป่วย</h3>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body">
                                <div id="example2_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <table role="grid" class="table">
                                                <tbody>
                                                    @{
                                                        foreach (var item in Model)
                                                        {
                                                            <tr role="row">
                                                                <td class="sorting_1">@item.Bed</td>
                                                                <td><a href="@Url.Action("Create", "Ward", new { HN = @item.HN })" style="color:blue">@item.HN</a></td>
                                                                <td><a href="@Url.Action("Create", "Ward", new { HN = @item.HN })" style="color:blue">@item.Name</a></td>
                                                            </tr>
                                                        }
                                                    }


                                                </tbody>

                                            </table>


                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- /.box-body -->
                        </div>

                    </div>
                </ul>

            </section>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper" style="min-height: 1136px;">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h4>
                    ข้อมูลผู้ป่วย
                </h4>
                @*<ol class="breadcrumb">
                        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                        <li><a href="#">Examples</a></li>
                        <li class="active">User profile</li>
                    </ol>*@
            </section>

            <!-- Main content -->
            <section class="content">

                <div class="row">
                    <div class="col-md-12">

                        <!-- Profile Image -->
                        <div class="box box-primary">
                            <div class="box-body box-profile" style="background-color:lightgray">
                                <div class="row">
                                    <div class="col-sm-12 center-block">
                                        <img class="profile-user-img img-responsive img-circle " src="~/dist/img/profile.png" alt="User profile picture">
                                    </div>
                                </div>
                                <div class="row">

                                    <div class="col-sm-12 center-block">

                                        @if (Model.Count == 0)
                                        {
                                            <div style="border-style:solid; border-color:lightslategrey ;padding:10px;">
                                                <div class="row ">
                                                    <div class="col-xs-6">
                                                        <b>HN : </b> <span id="HN"> </span>
                                                        <input hidden type="text" id="RegNo" value=" " />
                                                    </div>
                                                    <div class="col-xs-6">
                                                        <b>AN : </b> <span id="AN"> </span>
                                                    </div>

                                                </div>
                                                <div class="row">
                                                    <div class="col-xs-6">
                                                        <b>อายุ : </b> <span id="Age"></span>
                                                    </div>
                                                    <div class="col-xs-6">
                                                        <b>เตียง : </b> <span id="Bed"></span>
                                                    </div>

                                                </div>
                                                <div class="row">
                                                    <div class="col-xs-6">
                                                        <b>ชื่อ-สกุล :</b> <span id="Name"> </span>
                                                    </div>
                                                    <div class="col-xs-6">

                                                    </div>
                                                </div>
                                            </div>
                                        }
                                        else if (ViewBag.HN != null)
                                        {


                                            <div style="border-style:solid; border-color:lightslategrey ;padding:10px;">
                                                <div class="row">
                                                    <div class="col-xs-6">
                                                        <b>HN : </b> <span id="HN">@Model.Where(c => c.HN == ViewBag.HN).FirstOrDefault().HN</span>
                                                        <input hidden type="text" id="RegNo" value="@Model.Where(c=>c.HN==ViewBag.HN).FirstOrDefault().RegNo" />
                                                    </div>
                                                    <div class="col-xs-6">
                                                        <b>AN : </b> <span id="AN">@Model.Where(c => c.HN == ViewBag.HN).FirstOrDefault().AN</span>
                                                    </div>

                                                </div>
                                                <div class="row">
                                                    <div class="col-xs-6">
                                                        <b>อายุ : </b> <span id="Age">@Model.Where(c => c.HN == ViewBag.HN).FirstOrDefault().Age</span>
                                                    </div>
                                                    <div class="col-xs-6">
                                                        <b>เตียง : </b> <span id="Bed">@Model.Where(c => c.HN == ViewBag.HN).FirstOrDefault().Bed</span>
                                                    </div>
                                                    <div class="col-xs-6">

                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-xs-6">
                                                        <b>ชื่อ-สกุล :</b> <span id="Name">@Model.Where(c => c.HN == ViewBag.HN).FirstOrDefault().Name</span>
                                                    </div>
                                                    <div class="col-xs-6">

                                                    </div>
                                                </div>
                                            </div>

                                        }
                                        else
                                        {

                                            <div style="border-style:solid; border-color:lightslategrey ;padding:10px;">
                                                <div class="row ">
                                                    <div class="col-xs-6">
                                                        <b>HN : </b> <span id="HN">@Model.FirstOrDefault().HN</span>
                                                        <input hidden type="text" id="RegNo" value="@Model.FirstOrDefault().RegNo" />
                                                    </div>
                                                    <div class="col-xs-6">
                                                        <b>AN : </b> <span id="AN">@Model.FirstOrDefault().AN</span>
                                                    </div>

                                                </div>
                                                <div class="row">
                                                    <div class="col-xs-6">
                                                        <b>อายุ : </b> <span id="Age">@Model.FirstOrDefault().Age</span>
                                                    </div>
                                                    <div class="col-xs-6">
                                                        <b>เตียง : </b> <span id="Bed">@Model.FirstOrDefault().Bed</span>
                                                    </div>

                                                </div>
                                                <div class="row">
                                                    <div class="col-xs-6">
                                                        <b>ชื่อ-สกุล :</b> <span id="Name">@Model.FirstOrDefault().Name</span>
                                                    </div>
                                                    <div class="col-xs-4">

                                                    </div>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>

                                @*<a href="#" class="btn btn-primary btn-block"><b></b></a>*@
                            </div>
                            <!-- /.box-body -->
                        </div>
                    </div>
                </div>
                <!-- /.row -->
                <div class="box box-info">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="box-body">
                                <form class="form-horizontal">
                                    <div class="form-group center-block">
                                        <div class="col-xs-2 text-right">
                                            <label>เวร</label>
                                        </div>
                                        <div class="col-xs-3">
                                            <input type="radio" name="r1" id="Period1" value="ดึก" class="Period">
                                            <span class="margin">ดึก</span>

                                        </div>
                                        <div class="col-xs-3">
                                            <input type="radio" name="r1" id="Period2" value="เช้า" class="Period">
                                            <span class="margin">เช้า</span>

                                        </div>
                                        <div class="col-xs-3">
                                            <input type="radio" name="r1" id="Period3" value="บ่าย" class="Period">
                                            <span class="margin">บ่าย</span>
                                        </div>
                                        <div class="col-xs-1">

                                        </div>


                                    </div>
                                    <div class="form-group">
                                        <label for="inputName" class="col-sm-2 control-label ">วันที่เวลา</label>
                                        <div class="col-sm-4">
                                            <div class="input-group date">
                                                <div class="input-group-addon">
                                                    <i class="fa fa-calendar"></i>
                                                </div>
                                                <input type="text" class="form-control pull-right" id="Datepicker">
                                            </div>
                                        </div>
                                        <label for="inputName" class="col-sm-2 control-label ">เวลา</label>
                                        <div class="col-sm-4">

                                            <div class="input-group">
                                                <input type="text" class="form-control timepicker" id="Timepicker">

                                                <div class="input-group-addon">
                                                    <i class="fa fa-clock-o"></i>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="col-sm-2"></div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="box box-body">
                    <div class="row">
                        <div class="col-md-12">
                            <button type="button" id="btnViewAcceptOrder" onclick="window.open('@Url.Action( "ViewAcceptOrder", "Ward")')" class="btn btn-info ">บันทึกรับ Order</button>
                        </div>
                    </div><br />
                    <div class="row">
                        <!-- /.col -->
                        <div class="col-md-12">
                            <div class="nav-tabs-custom">
                                <ul class="nav nav-tabs">
                                    <li class="active"><a href="#vitalsign" id="tabVitalSign" data-toggle="tab" aria-expanded="true">Vital Sign</a></li>
                                    <li class=""><a href="#temp" id="tabTemp" data-toggle="tab" aria-expanded="false">Temp</a></li>
                                    <li class=""><a href="#body" id="tabBody" data-toggle="tab" aria-expanded="false">Body</a></li>
                                    <li class=""><a href="#pain" id="tabPain" data-toggle="tab" aria-expanded="false">Pain</a></li>
                                    <li class=""><a href="#fluid" id="tabFluid" data-toggle="tab" aria-expanded="false">Fluid</a></li>
                                    <li class=""><a href="#other" id="tabOther" data-toggle="tab" aria-expanded="false">Other</a></li>
                                </ul>
                                <div class="tab-content">


                                    <div class="tab-pane active" id="vitalsign">

                                        <br />
                                        <div class="row">
                                            <form class="form-horizontal">
                                                <div class="form-group">
                                                    <label for="inputName" class="col-sm-2 control-label">Systolic</label>

                                                    <div class="col-sm-10">
                                                        <input type="number" class="form-control MAPCal" id="inputSystolic" placeholder="Systolic">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="inputName" class="col-sm-2 control-label">Diastotic</label>

                                                    <div class="col-sm-10">
                                                        <input type="number" class="form-control MAPCal" id="inputDiastotic" placeholder="Diastotic">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="inputEmail" class="col-sm-2 control-label">MAP</label>

                                                    <div class="col-sm-10">
                                                        <input type="number" class="form-control" id="inputMAP" disabled="" placeholder="MAP">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="inputName" class="col-sm-2 control-label">Pulse</label>

                                                    <div class="col-sm-10">
                                                        <input type="number" class="form-control" id="inputPlus" placeholder="Pulse">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="inputName" class="col-sm-2 control-label">Respirations</label>

                                                    <div class="col-sm-10">
                                                        <input type="number" class="form-control" id="Respirations" placeholder="Respirations">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="inputEmail" class="col-sm-2 control-label">O2 sat</label>

                                                    <div class="col-sm-10">
                                                        <input type="number" class="form-control" id="inputO2Sat" placeholder="O2 sat">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="inputEmail" class="col-sm-2 control-label">O2 sat pre-duct</label>

                                                    <div class="col-sm-10">
                                                        <input type="number" class="form-control" id="inputO2SatPreduct" placeholder="O2 sat pre-duct">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="inputEmail" class="col-sm-2 control-label">O2 sat post-duct</label>

                                                    <div class="col-sm-10">
                                                        <input type="number" class="form-control" id="inputO2SatPostduct" placeholder="O2 sat post-duct">
                                                    </div>
                                                </div>

                                                <div class="form-group">
                                                    <div class="col-sm-offset-2 col-sm-12">
                                                        <button type="button" id="btnSaveVitalsign" class="btn btn-primary">Save</button>
                                                        <button type="button" id="btnClearVitalsign" class="btn btn-default">Clear</button>
                                                    </div>
                                                    <div class="col-sm-offset-2 col-sm-12">

                                                    </div>
                                                </div>
                                                <br />

                                                <div class="box box-info">
                                                    <div class="box-header with-border">
                                                        <h3 class="box-title">Chart</h3>

                                                        <div class="box-tools pull-right">
                                                            <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                                                <i class="fa fa-minus"></i>
                                                            </button>
                                                            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                                                        </div>
                                                    </div>
                                                    <div class="box-body chart-responsive ">
                                                        <div class="chart" id="line-chart-vitalsign" style="height: 300px; width:615px;  padding:25px;   "></div>
                                                    </div>
                                                    <!-- /.box-body -->
                                                </div>
                                            </form>
                                        </div>
                                    </div>



                                    <div class="tab-pane" id="temp">
                                        <form class="form-horizontal">
                                            <div class="form-group">
                                                <label for="inputName" class="col-sm-2 control-label">อุณหภูมิ (C)</label>

                                                <div class="col-sm-10">
                                                    <input type="number" class="form-control" id="inputTemp" placeholder="Temp">
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <div class="col-sm-offset-2 col-sm-12">
                                                    <button type="button" id="btnSaveTemp" class="btn btn-primary">Save</button>
                                                    <button type="button" id="btnClearTemp" class="btn btn-default">Clear</button>

                                                </div>
                                            </div>
                                        </form>
                                        <br />
                                        <div class="box box-info">
                                            <div class="box-header with-border">
                                                <h3 class="box-title">Chart</h3>

                                                <div class="box-tools pull-right">
                                                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                                        <i class="fa fa-minus"></i>
                                                    </button>
                                                    <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                                                </div>
                                            </div>
                                            <div class="box-body chart-responsive ">
                                                <div class="chart" id="line-chart-temp" style="height: 300px; width:615px;  padding:25px;   "></div>
                                            </div>
                                            <!-- /.box-body -->
                                        </div>

                                    </div>



                                    <div class="tab-pane " id="body">
                                        <form class="form-horizontal">


                                            <div class="form-group">
                                                <label for="inputName" class="col-sm-2 control-label">น้ำหนัก(kg)</label>

                                                <div class="col-sm-10">
                                                    <input type="number" class="form-control calBM1" id="inputWeight" placeholder="Weight">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label for="inputEmail" class="col-sm-2 control-label">ส่วนสูง(cm)</label>

                                                <div class="col-sm-10">
                                                    <input type="number" class="form-control calBM1" id="inputHeight" placeholder="Height">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label for="inputEmail" class="col-sm-2 control-label">BM1</label>

                                                <div class="col-sm-10">
                                                    <input type="number" class="form-control" id="inputBM1" disabled="disabled" placeholder="BM1">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label for="inputName" class="col-sm-2 control-label">รอบเอว(cm)</label>

                                                <div class="col-sm-10">
                                                    <input type="number" class="form-control" id="inputWaistline" placeholder="Waist">
                                                </div>
                                            </div>


                                            <div class="form-group">
                                                <label for="inputName" class="col-sm-2 control-label">ขนาดสะโพก(cm)</label>

                                                <div class="col-sm-10">
                                                    <input type="number" class="form-control" id="inputHip" placeholder="Hip">
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label for="inputName" class="col-sm-2 control-label">รอบศีรษะ(cm)</label>

                                                <div class="col-sm-10">
                                                    <input type="number" class="form-control" id="inputHead" placeholder="Head">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label for="inputName" class="col-sm-2 control-label">รอบอก(cm)</label>

                                                <div class="col-sm-10">
                                                    <input type="number" class="form-control" id="inputChest" placeholder="Chest">
                                                </div>
                                            </div>



                                            <div class="form-group">
                                                <div class="col-sm-offset-2 col-sm-12">
                                                    <button type="button" id="btnSaveBody" class="btn btn-primary">Save</button>
                                                    <button type="button" id="btnClearBody" class="btn btn-default">Clear</button>
                                                </div>
                                            </div>
                                            <br />
                                            <div class="box box-info">
                                                <div class="box-header with-border">
                                                    <h3 class="box-title">Chart</h3>

                                                    <div class="box-tools pull-right">
                                                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                                            <i class="fa fa-minus"></i>
                                                        </button>
                                                        <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                                                    </div>
                                                </div>
                                                <div class="box-body chart-responsive ">
                                                    <div class="chart" id="line-chart-weight" style="height: 300px; width:615px;  padding:25px;   "></div>
                                                </div>
                                                <!-- /.box-body -->
                                            </div>
                                        </form>
                                    </div>


                                    <div class="tab-pane " id="pain">


                                        <form class="form-horizontal">
                                            <div class="timeline-body form-group">
                                                <label for="inputName" class="col-sm-2 control-label">Pain Score</label>

                                                <div class="col-sm-10">
                                                    <label class="margin">
                                                        <input type="radio" name="r1" value="1" class="minimal" checked>
                                                        <span class="margin" id="inputPain1">1</span>
                                                    </label>

                                                    <label class="margin">
                                                        <input type="radio" name="r1" value="2" class="minimal">
                                                        <span class="margin" id="inputPain2">2</span>
                                                    </label>
                                                    <label class="margin">
                                                        <input type="radio" name="r1" value="3" class="minimal">
                                                        <span class="margin" id="inputPain3">3</span>
                                                    </label>
                                                    <label class="margin">
                                                        <input type="radio" name="r1" value="4" class="minimal">
                                                        <span class="margin" id="inputPain4">4</span>
