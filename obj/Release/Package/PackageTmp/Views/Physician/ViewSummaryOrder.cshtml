            var url = '@Url.Action("GetVitalSign", "Ward")';
            $.getJSON(url, { AN: ANVal }, function (result) {
              // var jason_data = JSON.stringify(result);

                new Morris.Line({
                    element: 'line-chart-vitalsign',
                // Chart data records -- each entry in this array corresponds to a point on
                // the chart.
                    data: result,
                // The name of the data record attribute that contains x-values.
                    xkey: 'Seq',
                    parseTime: false,
                // A list of names of data record attributes that contain y-values.
                    ykeys: ['ValueSystolic', 'ValueDiastotic', 'ValMAP', 'ValO2Sat', 'ValO2SatPreduct', 'ValO2SatPostduct', 'ValPlus','ValRespirations'],
                // Labels for the ykeys -- will be displayed when you hover over the
                // chart.
                    labels: ['ValueSystolic', 'ValueDiastotic', 'ValMAP', 'ValO2Sat', 'ValO2SatPreduct', 'ValO2SatPostduct', 'ValPlus', 'ValRespirations'],
                    lineColors: ['#3c8dbc','red','green','blue','pink','purple','orange','gray']
            });
        });
        }
