        $("#upload").click(function () {
            var AN = $("#AN").text();
            var HN = $("#HN").text();
            var RegNo = $("#RegNo").val();
            var data = new FormData();



            $.ajax({
                url: '@Url.Action("CheckOrderStatus", "Physician")',
                data: {
                    HN: HN,
                    AN: AN,
                    RegNo: RegNo
                },
                type: "GET",
                async:false,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    //table display data

                    if (response.Flag) {
                        alert("มี order ค้างอยู่ไม่สามารถสร้าง order ใหม่ได้");

                        return;
                    } else {
                        //Add the Multiple selected files into the data object
                        var ProgessNote = $("#ProgessNote").val();
                        var OrderOneDay = $("#OrderOneDay").val();
                        var OrderContinue = $("#OrderContinue").val();



                        var files = $("#files").get(0).files;
                        for (i = 0; i < files.length; i++) {
                            data.append("files" + i, files[i]);
                        }


                        if (ProgessNote == "" && OrderOneDay == "" && OrderContinue == "" && files.length==0) {
                            alert("ยังไม่ได้ป้อนข้อมูล หรือ upload file");
                            return false;
                        }



                        data.append("AN", AN);
                        data.append("HN", HN);
                        data.append("RegNo", RegNo);
                        data.append("ProgessNote", ProgessNote);
                        data.append("OrderOneDay", OrderOneDay);
                        data.append("OrderContinue", OrderContinue);

                        //Post the data (files) to the server

                        $.ajax({
                            type: 'POST',
                            url: "@Url.Action("Upload")",
                            data: data,
                            contentType: false,
                            processData: false,
                            async:false,
                            success: function (data) {
                                if (data.Flag) {
                                    alert("สร้าง Order เรียบร้อยแล้ว");
                                    location.reload();
                                } else {
                                    alert("เกิดปัญหาในการบันทึกข้อมูล");
                                }

                            },
                            error: function () {
                                alert("เกิดปัญหาในการบันทึกข้อมูล1" + data.Message);
                                location.reload();
                            },
                        });
                    }
                }

            });
        });



