                    </div>
                </div>


                <ul class="sidebar-menu tree" data-widget="tree">


                    <div class="box-body">


                        <div class="box" style="background-color:lightgray; width:245px">
                            <div class="box-header">
                                <h3 class="box-title">รายชื่อ Lab</h3>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body">
                                <div id="example2_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <input hidden type="text" id="req_no" />
                                            <input hidden type="text" id="labName" />
                                            <table role="grid" class="table">
                                                <tbody>
                                                    @{
                                                        foreach (var item in Model.labMenuHeads)
                                                        {
                                                            <tr role="row">
                                                                <td>
                                                                    <strong class="menuLab" style="cursor:pointer; color:blue" value="@item.lab_menu_name" id="@item.req_no">@item.lab_menu_name</strong>

                                                                </td>
                                                            </tr>
                                                            foreach (var detail in item.labMenuDetails)
                                                            {
                                                                <tr role="row">
                                                                    <td>&nbsp;&nbsp;&nbsp;@detail.lab_name</td>
                                                                </tr>
                                                            }
                                                        }
                                                    }

                                                </tbody>

                                            </table>


                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- /.box-body -->
                        </div>

                    </div>
                </ul>

