<script type="text/javascript">
    $(function () {

        $(".menuLab").click(function () {
            var req_no = this.id;
            var HN = $("#HN").text();
            var RegNo = $("#RegNo").val();
            var labName = $(this).text();


            $("#req_no").val(req_no);

            $("body").css("cursor", "progress");
            $.ajax({
                url: '@Url.Action("GetLabResult", "Physician")',
                data: {
                    HN: HN,
                    RegNo: RegNo,
                    req_no: req_no
                },
                type: "GET",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    //table display data
                    var html = "";
                    $("#divLabResult").empty();

                    html += "&nbsp;&nbsp;&nbsp;<strong  style='color: blue' >" + labName + "</strong></br>" +
                        "<table class='table no-margin' > " +
                        "<thead><tr>" +
                        "<th style='width: 30%'>ชื่อผล Lab</th>" +
                        "<th style='width: 30%'>ผล Lab</th>" +
                        "<th style='width: 40%'>Reference Rage</th>" +
                        "</tr></thead><tbody>";
                    jQuery.each(response, function (index, item) {
                        html += "<tr>"
                        if (item.IsTextRed) {
                            html += "<td class='text-red'>" + item.result_name + "</td>" +
                                "<td class='text-red'>" + item.real_res + "</td>" +
                                "<td>" + item.ReferenceRate + "</td></tr>"
                        } else {
                            html += "<td>" + item.result_name + "</td>" +
                                "<td>" + item.real_res + "</td>" +
                                "<td>" + item.ReferenceRate + "</td></tr>"
                        }
                    });
                    html += "</tbody> </table>";

                    $("#divLabResult").append(html);

                }

            });


             $.ajax({
                url: '@Url.Action("GetLabCompare", "Physician")',
                data: {
